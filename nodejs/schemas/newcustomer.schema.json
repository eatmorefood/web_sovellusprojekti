{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "New customer schema",
    "description": "Schema for a new restaurant app customer",
    "default": {},
    "examples": [
        {
            "fname": "Mac",
            "lname": "Gyver",
            "email": "mac@gyver.com",
            "phone": 1234567890,
            "address": "Macgyver street 1",
            "password": "MacGyver123"
        }
    ],
    "required": [
        "fname",
        "lname",
        "email",
        "phone",
        "address",
        "password"
    ],
    "properties": {
        "fname": {
            "$id": "#/properties/fname",
            "type": "string",
            "title": "The fname schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Mac"
            ]
        },
        "lname": {
            "$id": "#/properties/lname",
            "type": "string",
            "title": "The lname schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Gyver"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "mac@gyver.com"
            ]
        },
        "phone": {
            "$id": "#/properties/phone",
            "type": "integer",
            "title": "The phone schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1234567890
            ]
        },
        "address": {
            "$id": "#/properties/address",
            "type": "string",
            "title": "The address schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Macgyver street 1"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "MacGyver123"
            ]
        }
    },
    "additionalProperties": false
}