{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "restID": 1,
            "custID": "5a090e5c-6423-4096-9bfb-eeb202e47b04",
            "foodID": "2",
            "total": 30.0,
            "address": "Bring me some food 6 B 23"
        }
    ],
    "required": [
        "restID",
        "custID",
        "foodID",
        "total",
        "address"
    ],
    "properties": {
        "restID": {
            "$id": "#/properties/restID",
            "type": "integer",
            "title": "The restID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "custID": {
            "$id": "#/properties/custID",
            "type": "string",
            "title": "The custID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "5a090e5c-6423-4096-9bfb-eeb202e47b04"
            ]
        },
        "foodID": {
            "$id": "#/properties/foodID",
            "type": "string",
            "title": "The foodID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "2"
            ]
        },
        "total": {
            "$id": "#/properties/total",
            "type": "number",
            "title": "The total schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                30.0
            ]
        },
        "address": {
            "$id": "#/properties/address",
            "type": "string",
            "title": "The address schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Bring me some food 6 B 23"
            ]
        }
    },
    "additionalProperties": false
}