{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "idfood": "1",
            "name": "Best burger",
            "category": "Burgers",
            "description": "Best burger in the world",
            "price": 12.3,
            "idrestaurant": 1,
            "image": "path"
        }
    ],
    "required": [
        "idfood",
        "name",
        "category",
        "description",
        "price",
        "idrestaurant",
        "image"
    ],
    "properties": {
        "idfood": {
            "$id": "#/properties/idfood",
            "type": "string",
            "title": "The idfood schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Best burger"
            ]
        },
        "category": {
            "$id": "#/properties/category",
            "type": "string",
            "title": "The category schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Burgers"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Best burger in the world"
            ]
        },
        "price": {
            "$id": "#/properties/price",
            "type": "number",
            "title": "The price schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0.0,
            "examples": [
                12.3
            ]
        },
        "idrestaurant": {
            "$id": "#/properties/idrestaurant",
            "type": "integer",
            "title": "The idrestaurant schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "image": {
            "$id": "#/properties/image",
            "type": "string",
            "title": "The image schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "path"
            ]
        }
    },
    "additionalProperties": false
}